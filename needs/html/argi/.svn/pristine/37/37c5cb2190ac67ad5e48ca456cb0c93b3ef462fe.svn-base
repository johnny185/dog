//获取位置信息******************************
function getLocation(map){
		map.getLocation({
			accuracy : '10m',
			autoStop : true,
			filter : 1
		}, function(ret, err) {//localStorage.clear();
			if (ret.status) {
				var lon = ret.lon;
				var lat = ret.lat;

//					if (cacheWeather == null) {//alert(1);
//						getWeatherMsg(lon, lat);
//					} else {//alert(2)
//						var nowTime = new Date();
//						var cacheTime = 1800;
//						// 缓存时间半小时
//						// 缓存时间
//						var oldTime = localStorage.getItem(oldWeatherTimeKey);
//						if ((nowTime.getTime() - oldTime) / 1000 > cacheTime) {
//							//alert(3)
//							// 重新获取天气数据
//							getWeatherMsg(lon, lat);
//						} else {//alert(4)
//							// 在缓存时间内展示旧数据
//							showWeatherMsg(JSON.parse(cacheWeather));
//							$(".home_weather_info p:last").text(cacheAddress);
//						}
//					}
				getWeatherMsg(lon, lat);
//					获取区县
				map.getNameFromCoords({
					lon : lon,
					lat : lat
				}, function(ret, err) {
//					alert(JSON.stringify(ret))
					if (ret.status) {
						// 得到区县名
						$(".home_weather_info p:last span:last").text(ret.district);
//						得到市区名
						$(".home_weather_info p:last span:first").text(ret.city);
//							localStorage.setItem(addressKey, ret.district);
					}
					map.stopLocation();
				});
				
			} else {
				api.alert({msg: "获取定位信息失败"});
			}
		});

}

//公告轮播*********************************************************************	
function publickInfoTransfer(){
	var wrap2ListSum=2;  //此数据根据数据库轮播图个数决定
	var wrap2Width=$("#top>.noticeBanner>.bannerWrap2 .bannerWrap2_center").width();
	$("#top>.noticeBanner>.bannerWrap2 .bannerWrap2_center .swiper-wrapper").width(wrap2Width*(wrap2ListSum+1));
	$("#top>.noticeBanner>.bannerWrap2 .bannerWrap2_center .swiper-wrapper .swiper-slide").css({
		"-webkit-box-flex": "1",
		"-webkit-flex": "1",
		"flex": "1",
	});
	var marginLeftAdd=0;
	function wrap2Move(){
		marginLeftAdd+=3;
		if(marginLeftAdd>=450*wrap2ListSum){
			marginLeftAdd=0
		};
		$("#top>.noticeBanner>.bannerWrap2 .bannerWrap2_center .swiper-wrapper").css({
			"marginLeft":-1*marginLeftAdd/100+"rem"
		})
	};
	var wrap2Timer=setInterval(wrap2Move,30);
	$("#top>.noticeBanner>.bannerWrap2 .bannerWrap2_center").on("touchstart",function(){
		clearInterval(wrap2Timer);
	}).on("touchend",function(){
		clearInterval(wrap2Timer);
		wrap2Timer=setInterval(wrap2Move,30);
	})

}
	
