    <style>
    	*{
			box-sizing: border-box;
		}
		html,body{
			width:100%;
			height:100%;
		}
		body{
			position: relative;
			overflow-y: auto;
			background-color: #f2f2f2;
			padding-top: 1.28rem;
		}
		
		#top{
			width: 100%;
			height: 0.46rem;
			background: #00c558;
			margin-bottom: 0.82rem;
			position: fixed;
			left: 0;
			top: 0;
			z-index: 99;
		}
		header{
			position: fixed;
			left: 0;
			top: 0.44rem;
			width: 100%;
			background: #00c558;
			z-index: 99;
		}
		header #title{
			position: relative;
			width:100%;
			height: 0.82rem;
			line-height: 0.82rem;
			text-align: center;
			font-size: 0.36rem;font-family: PingFangSC-Regular, sans-serif;color: #fff;
		}
		header #title .left{
			display: inline-block;
			position: absolute;
			left: 0.25rem;
			bottom: 0.28rem;
			width: 0.16rem;
			height: 0.28rem;
			background-image: url(../../../../image/harvest_back.png);
			background-position: center;
			background-size: 100% 100%;
			background-repeat: no-repeat; 
		}
		#content{
			padding: 0.77rem 0.3rem 0 0.84rem;
		}
		#content>div{
			width: 100%;
		}
		#content>div>ul li{
			position: relative;
			width: 100%;
			padding: 0.17rem 0;
			line-height: 1.1rem;
			margin-bottom: 0.58rem;
			border: solid 1px #00c558;
			padding: 0.17rem 0.25rem 0.17rem 0.93rem;
		}
		#content>div>ul li>p{
			line-height: 0.32rem;
			font-size: 0.29rem;font-family: PingFangSC-Regular, sans-serif;color: #40d10f;
		}
		#content>div>ul li>div{
			position: absolute;
			left: -0.38rem;
			top: -0.1rem;
			height: 0.93rem;
			text-align: center;
			line-height: 0.93rem;
			width: 0.93rem;
			background: #00c558;
			border-radius: 50%;

		}
		#content>div>ul li>div>span:first-of-type{
			position: relative;
			top: -0.28rem;
			font-size: 0.25rem;font-family: PingFangSC-Regular, sans-serif;color: #fff;
		}
		#content>div>ul li>div>span:last-of-type{
			position: absolute;
			left: 0.13rem;
			top: -0.44rem;
			width: 0.66rem;
			height: 0.58rem;
			background-image: url(../../../../image/harvest_gradeStandard_bc1.png);
			background-repeat: no-repeat;
			background-size: 100% 100%;
			background-position: center; 
		}
