html, body{
	font-family: "Helvetica Neue", "Roboto", "Segoe UI", sans-serif;
	background-color: #fafafa;
	width: 100%;
	overflow-x:hidden;
	overflow-y: scroll;
	-ms-word-wrap: break-word; 
	word-wrap: break-word;
	-webkit-text-size-adjust: none; 
	text-size-adjust: none;
	}

#main-menu > div > a {
	width: 100%;
}

.height100{
	height: 100%;
}

/*-------Layout------------*/
.container{
	top:0px;
	margin-top: 0px;
}
.sub-container-notop{
	margin-top: 52px;
}
.sub-container-nobottom{
	margin-bottom: 44px;
	bottom: 44px;
}
.main{
	margin: 0px 20px;
}
.main-10{
	margin: 0px 10px;
}
.full-main{
	margin: 0px;
}

/*--------features----------*/
.clear{
	clear: both;
}
.fixed{
	position: fixed;
}
.center{
	margin-left: auto;
	margin-right: auto;
	text-align: center;
}
.left{
	float: left;
}
.right{
	float: right;
}

/*--------banner----------*/
.banner{
	width: 100%;
	height: 52px;
	padding-top: 10px;
	line-height: 44px;
	top: 0;
	z-index: 99999;
	border-bottom: 1px solid #bfbfbf;
	color: #333;
	background-color: #f0f0f0;
}
.banner-btn{
	/*border: 1px solid #fff;*/
	width: 20px;
	height: 20px;
	position: absolute;
	/*margin: -64px 0px 0px 20px; */
}
.back-btn{
	width: 20px;
	height: 20px;
	margin: 5px 0px 0px 20px;
}
.banner-btn-r{
	width: 20px;
	height: 20px;
	position: absolute;
	margin: -64px 20px 0px 88%; 
}
.banner-back-btn{
	top: 22px;
    left: 20px;
	background-image: url("../image/back-button.png");
	background-size: 100%;
	background-repeat: none;
}
.banner-edit-btn{
	background-image: url("../image/edit.png");
	background-size: 100%;
	background-repeat: none;
}
.banner-title{
	text-align: center;
	margin: auto;
}
.banner-title h4{
	color: #333;
	font-size: 18px;
	font-weight: normal;
}
.notify-para{
	/*border: 1px solid red;*/
	width: 80%;
	margin: 40px auto -60px ;
}
.notify-para h4{
	padding-top: 20px;
	color: red;
}
/*--------banner2----------*/
.banner2{
	width: 100%;
	height: 44px;
	background: #fafafa;
	border-bottom: 1px solid #eee;
	line-height: 44px;
	top: 0;
	z-index: 99999;
}
/*--------banner-nocolor----------*/
.banner-nocolor{
	position: relative;
	width: 100%;
	height: 52px;
	padding-top: 8px;
	background: none
	border-bottom: 1px solid #eee;
	line-height: 44px;
	top: 0;
	z-index: 99999;
}
/*--------navbar----------*/
.nav{
	bottom: 0px;
	width: 100%;
	height: 44px;
	z-index: 99999;
	background-color: #fff;
}

/*---------menu-----------*/
.menu1st{

}
.menu1st ul{
	list-style: none;
	margin: 0px 0px; 
	background-color: none;
    margin-bottom: 3px;
     -webkit-box-shadow: 0px 2px 4px #dddcdc;
       -moz-box-shadow: 0px 2px 4px #dddcdc;
    		box-shadow: 0px 2px 4px #dddcdc;
}
.menu1st ul li{
	width: 100%;
    height: 53px;
    line-height: 53px;    
}
.menu1st ul li div{
	border-bottom: 1px solid #d9d9d9;
}
.menu1st ul li a{
	width: 100%;
}
.menu1st ul li div h5{
	font-size: 16px;
	font-weight: normal;
	color: #333;
}
.menu1st ul li div .next-block{
	margin-top: -54px;
	clear: both;
	display: inline-block;
	float: right;
}
.menu1st ul li div .nextBtn{
	position: absolute;
	right: 0px;
	height: 44px;
	margin-right: 22px;
	padding-top: 6px;
}
.nextBtn .sm-button{
	width: 24px;
	height: auto;
}
/*--------button----------*/
.btn-1{
	margin-top: 10px;
	width: 100%;
	height: 44px;
	line-height: 44px;
	background-color: rgb(18,139,3);
}
.btn-2{
	margin-top: 10px;
	width: 100%;
	height: 44px;
	line-height: 44px;
	border: 1px solid rgb(18,139,3);
	background-color: #f0f0f0;
}
.btn-3{
	margin-top: 20px;
	width: 100%;
	height: 44px;
	line-height: 44px;
	background-color: #66BB6A;
}
.btn-1-text{
	color: #fff;
}
.btn-1-text2{
	color: rgb(18,139,3);
}
.btn-1:active {
	opacity: 0.5;
}
.btn-2:active {
	opacity: 0.5;
}

[class*="col-"]{
	position: relative;
}
.col-sm-1{
	width: 6%;
}
.col-1{
	width: 10%;
}
.col-lg-1{
	width: 15%;
}
.col-sm-2{
	width: 20%;
}
.col-2{
	width: 28%;
}
.col-3{
	width: 30%;
}
.col-4{
	width: 33.3%;
}
.col-lg-4{
	width: 45%;
}
.col-5{
	width: 50%;
}
.col-6{
	width: 62%;
}
.col-7{
	width: 70%;
}
.col-8{
	width: 80%;
}


#width100{
	width: 100%;
}

.harvest-margin-bottom{
    margin-bottom: 44px;
}
.margin-l-20{
	margin-left: 20px;
}

/*------------------------*/

input[type="button"], input[type="submit"], input[type="reset"],input[type="textarea"]{
	-webkit-appearance: none;
	-webkit-border: none;
	outline:none;
	resize: none;
}
input:focus{
	-webkit-appearance: none;
	-webkit-border: none;
	outline:none;
	resize: none;
}
input:active{
	-webkit-appearance: none;
	-webkit-border: none;
	outline:none;
	resize: none;
}