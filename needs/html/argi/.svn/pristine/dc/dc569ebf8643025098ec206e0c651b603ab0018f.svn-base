				data:{"data":JSON.stringify(farmingDataResource)},
				success:function(e){								 						 				 		
				 		
				 	var resourceInfo=e.data;				 											
						 	 if(resourceInfo != null){									  				 							    
							    							  								
								$("#grade").val(resourceInfo.resourceType);	
								var id = resourceInfo.resourceType;										
