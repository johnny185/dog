<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
    <meta name="format-detection" content="telephone=no,email=no,date=no,address=no">
    <title>审批意见总监页</title>
    <link rel="stylesheet" href="../../../css/aui.css" />
    <link rel="stylesheet" href="../../../css/aui_bar.css" />
    <style>
    	
    	.aui-pull-left span{
  			display:block;
  			width:15px;
  			height:18px;
  			background:url("../../../image/business_img/back.png") no-repeat;
	 		background-size:100% 100%;
  		}
  		.aui-bar-nav .aui-pull-left {
	    	width: 50px;
	    }
    </style>
</head>
<body>
	    
	
     <header class="aui-bar aui-bar-nav" id="aui-header">
        <a class="aui-btn aui-pull-left" tapmode onclick="closeWin()">
            <span ></span>
        </a>
        <div class="aui-title" >审批意见</div>
      	<a class="aui-pull-right aui-btn aui-btn-outlined" tapmode onclick="makeSure()">
            <span >确定</span>
        </a> 
     </header>
     
	    
	    
<script type="text/javascript" src="../../../script/api.js"></script> 
</body>

<script type="text/javascript">
    apiready = function(){
    api.addEventListener({
	    name:'toCloseChief'
    },function(ret,err){
    	api.sendEvent({
	        name:'CloseChief'
        });
    });
        api.parseTapmode();
        var header = $api.byId('aui-header');
        $api.fixStatusBar(header); 
        var headerPos = $api.offset(header);
        var body_h = $api.offset($api.dom('body')).h;
        api.openFrame({
            name: 'chiefopin_frm',
            url: './chiefopin_frm.html',
            bounces: true,
            pageParam:api.pageParam,
            reload:true,
            rect: {
                x: 0,
                y: headerPos.h,
                w: 'auto',
                h: 'auto'
            }
        })
        
    };
    function closeWin(){
//  	api.closeToWin({
//	        name: 'chief_win'
//      });
        api.closeWin({
        });
    }
   function makeSure() {
   	api.sendEvent({
	    name: 'onMakeSure',
	    extra: {}
	});
   }
</script>
</html>