<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
    <meta name="format-detection" content="telephone=no,email=no,date=no,address=no">
    <title>客户拜访</title>
    <link rel="stylesheet" href="../../../css/aui.css" />
    <link rel="stylesheet" href="../../../css/aui_bar.css" />
    <style>
    
   		.aui-pull-left span{
  			display:block;
  			width:15px;
  			height:18px;
  			background:url("../../../image/business_img/back.png") no-repeat;
	 		background-size:100% 100%;
  		}
  		.aui-bar-nav .aui-pull-left {
	    	width: 50px;
	    }
    </style>
</head>
<body>
	    
	
     <header class="aui-bar aui-bar-nav" id="aui-header">
        <a class="aui-btn aui-pull-left" tapmode onclick="closeWin()">
            <span></span>
        </a>
        <div class="aui-title" >我的客户</div>
        <a class="aui-pull-right aui-btn aui-btn-outlined" onclick="punchRecord()">
            <span >新建</span>
        </a>
     </header>
     
	    
	    
<script type="text/javascript" src="../../../script/api.js"></script>

</body>

<script type="text/javascript">
    apiready = function(){
        api.parseTapmode();
        var header = $api.byId('aui-header');
        $api.fixStatusBar(header); 
        var headerPos = $api.offset(header);
        var body_h = $api.offset($api.dom('body')).h;
        if(typeof (api.pageParam)==='object'){
          api.pageParam=api.pageParam.roleType
        }
        console.log(JSON.stringify(api.pageParam))
        api.openFrame({
            name: 'visit_frm',
            url: 'visit_frm.html',
            bounces: true,
            pageParam:api.pageParam,
            rect: {
                x: 0,
                y: headerPos.h,
                w: 'auto',
                h: 'auto'
            }
        })
    };
    function closeWin(){
      console.log(JSON.stringify(api.pageParam))
      if(typeof (api.pageParam)==='object'){
          api.pageParam=api.pageParam.roleType
        }
      if(api.pageParam===0){
          api.openWin({
          name: 'sales_win',
          url: './sales_win.html',
          reload: true,
          pageParam:api.pageParam
       })
      }else if(api.pageParam===1){
       api.openWin({
          name: 'chief_win',
          url: './chief_win.html',
          reload: true,
          pageParam:api.pageParam
       })
      }else{}
    }
    function punchRecord(){
      api.openWin({
          name: 'infor_win',
          url: './infor_win.html'
       })
    }
</script>
</html>