<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
    <meta name="format-detection" content="telephone=no,email=no,date=no,address=no">
    <title>基本信息</title> 
    <link rel="stylesheet" href="../../../css/public.css" />
    <link rel="stylesheet" href="../../../css/actionsheet.css" />
    <script type="text/javascript" src="../../../script/rem.js"></script>
    <script type="text/javascript" src="../../../script/remote.config.js"></script>
 	<style>
	 	.content{
	 		font-size: 0.27rem;
	 		color:#706969;
	 		position:relative;
	 		background-color:#f2f2f2;
	 	} 	
	 	.top_img{
	 		width: 100%;
	 		height:2.33rem;
	 		background:url("../../../image/business_img/b_b.png") no-repeat;
	 		background-size:100% 100%;
	 	}
	 	.text{
	 		width: 6.79rem;
	 		overflow:hidden;
	 		background-color:#fff;
	 		margin:0 auto;
	 		position:relative;
	 		top:-1.29rem;
	 		border-radius:0.1rem;
	 	}
	 	.basic{
	 		margin:0.47rem 0 0.1rem;
	 		width:1.38rem;
	 		height:0.4rem;
	 		line-height:0.4rem;
	 		color:#FFF;
	 		background-color:#66bd14;
	 		border-bottom-right-radius: 0.2rem;
	 		border-top-right-radius: 0.2rem;
	 		text-align:center;
	 	}
		.item{
	 		width:100%;
	 		min-height:0.77rem;
	 		line-height:0.77rem;
	 		border-bottom:1px solid #f2f2f2;
	 		padding: 0 0.23rem;
	 		box-sizing: border-box;
	 		overflow: hidden;
	 		display:-webkit-flex;
	 		display:flex;
	 	}
	 	.input{
	 		height:0.77rem;
	 		line-height:0.77rem;
	 		color:#706969;
	 		padding-left:0.1rem;
	 		box-sizing: border-box;
	 		flex:1;
	 		-webkit-flex:1;
	 	}
	 	.phone .input{
	 		margin-right:0.8rem;
	 	}
	 	.phone .input.input_h{
	 		margin-right:0;
	 	}
	 	.item .right.location{
	 	  width: 0;
          height: 0;
          overflow: hidden;
          padding:0;
	 	}
	 	.text .item:last-child{
	 		border:none;
	 	}
	 	.item div,.item span,input{
	 		display:inline-block;
	 		font-size: 0.27rem;
	 	}
	 	.item .left{
	 		min-width:1.2rem;
	 	}
	 	.item .inner{
	 		width:3.4rem;
	 		padding-left:0.1rem;
	 		flex:1;
	 		-webkit-flex:1;
	 	}
	 	.item .right{
	 		float:right;
	 		background:url("../../../image/business_img/b_add.png") no-repeat left center;
	 		background-size:0.3rem;
	 		padding-left:0.4rem;
	 		box-sizing:border-box;
	 		height:0.77rem;
	 	}
	 	.item .check{
	 		float:right;
	 	}
	 	.phone{
	 		overflow: hidden;
	 		position:relative;
	 	}
	 	.phone .import{
	 		position:absolute;
	 		right:0;
	 		top: 50%;
	 		margin-top:-0.4rem;
	 		width:0.8rem;
	 		text-align:left;
	 		background:url("../../../image/business_img/b_in.png") no-repeat top center;
	 		background-size:0.38rem 0.38rem;
	 		padding-top:0.4rem;
	 		box-sizing:border-box;
	 		background-color:#fff;
	 	}
	 	
	 	.check_out{
	 		position:absolute;
	 		top:0;
	 		left:0;
	 		background:rgba(0,0,0,0.3);
	 		width: 100%;
	 		height:100%;
	 		z-index: 1000;
	 		text-align:center;
	 	}
	 	.select{
	 		width: 3rem;
	 		height:0.5rem;
	 		overflow:hidden;
	 		background-color:#fff;
	 		margin-top:50%;
	 	}
	 	.item .text_r {
	 		text-align:right;
	 	}
	 	.item .text_l {
	 		text-align:left;
	 	}
	 	.point{
	 		margin-left:0.2rem;
	 	}
	 	
	 	/*textarea提示信息字颜色*/		
		.input::-webkit-input-placeholder {
		  color: #d1cdcd;
		}
		.input:-moz-placeholder {
		   color: #d1cdcd;
		}
		.input:-ms-input-placeholder {
		   color: #d1cdcd;
		}
    </style>
</head>
<body>
  		<div class="content" id="content">
  			<div class="top_img"></div>
			<div class="text">
				<div class="basic">基本信息</div>
				
				<div class="item">
					<span class="left">客户名称</span>
					<input type="text" id="customerName" class="input" maxlength="50" placeholder="请输入客户名称（必填）" value=""/>
				</div>
				<div class="item">
					<span class="left">地址</span>
					<!--地址-->
					 <input type="hidden" id="provinceCode" value=""/>
                     <input type="hidden" id="cityCode" value=""/>
                     <input type="hidden" id="countyCode" value=""/>
					<span class="inner"id="provinceCityCounty"></span>
					<div class="right"id="location">
						点我定位
					</div>
				</div>
				<div class="item">
					<span class="left">详细地址</span>
					<input type="text" id="address"class="input" maxlength="50" placeholder="请输入详细地址" value=""/>
				</div>
				<!--区域-->
				<div class="item">
					<span class="left">所属区域</span>
					<!--区域-->
					<span class="inner" id="districtval"></span>
					<input type="hidden"  id="district" value=""/>
					<div class="areaSelect point" style="display: none;">></div>
					<div class="check" id="areaSelect">
						请选择>
					</div>
										
				</div>
				<!--负责人及电话-->		
				<div class="phone">
					<div class="item">
						<span class="left">负责人</span>
						<input type="text" id="principal" class="input" maxlength="10" placeholder="请输入负责人（必填）" value=""/>
					</div>
					<div class="item">
						<span class="left">联系电话</span>
						<input type="text" id="phone" class="input" placeholder="请输入负责人电话（必填）" value=""/>
					</div>
					<div class="import" id="import">
						导入
					</div>
				
				</div>	
				<!--公司性质-->
				<div class="item">
					<span class="left">公司性质</span>
					<!--公司性质-->
					<span class="inner" id="companyNatureval"></span>
					<input type="hidden" id="companyNature" value="" />
					<div class="companynature point" style="display: none;">></div>
					<div class="check" id="companynature">
						请选择>
					</div>
				</div>	
				
				<div class="item">
						<span class="left">法人姓名</span>
						<input type="text"id="legalPerson" class="input" maxlength="10" placeholder="请输入法人姓名" value=""/>
				</div>
				<div class="item">
						<span class="left">主要经营范围</span>
						<input type="text" id="mainBusiness"class="input" maxlength="200" placeholder="请输入主要经营范围" value=""/>
				</div>
			
				<div class="item">
					<span class="left">冷库面积</span>
					<input type="number"  id="coldStorage" 
					onkeyup= "minus(this)"
                    class="input" placeholder="请输入冷库面积" value=""/>
					<div class="area">
						平方米
					</div>
				</div>
				<!--客户分类-->
				<div class="item">
					<span class="left">客户分类</span>
					<!--客户分类-->
					<span class="inner" id="customerClassval"></span>
					<input type="hidden" id="customerClass" value="" />
					<div class="customerKinds point" style="display: none;">></div>
					<div class="check" id="customerKinds">
						请选择>
					</div>
				</div>
			</div>
  		</div>
