}
.textareas {
	padding: 10px;
}
.radioSlipCss3 {
	padding-bottom: 10px;
	overflow: hidden;
}
.radioSlipCss3 > div {
	display: inline-block;
	line-height: 26px;
	float: left;
	margin-right: 10px;
}
.radioSlip{
	position: relative;
	cursor: pointer;
	width: 60px;
	height: 26px;
	overflow: hidden;
} 
.radioSlipFalse,
.radioSlipTrue {
	position: absolute;
	top: 0;
	width: 60px;
	height: 26px;
	border-radius: 26px;
}
.radioSlipFalse {
	left: 0;
	background-color: #f5f5f5;
	z-index: 2;
}
.radioSlipTrue {
	left: 0;
	background-color: #44c24a;
	z-index: 1;
	display: none;
}
.radioSlipFalse:after,
.radioSlipTrue:after {
	display: inline-block;
	position: absolute;
	content: "";
	width: 24px;
	height: 24px;
	border-radius: 24px;
	box-shadow: 0 3px 2 #ddd;
}
.radioSlipFalse:after{
	left:2px;
	top: 1px;
	background-color: #fff;
}
.radioSlipTrue:after{
	right:2px;
	top: 1px;
	background-color: #fff;
}