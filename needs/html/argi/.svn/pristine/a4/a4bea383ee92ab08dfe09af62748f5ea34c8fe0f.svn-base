}
.listBlockMax {
	margin-bottom: 10px;
	box-shadow: 0 2px 2px #ddd;
}
.listBlockMax-title {
	background-color: #f5f5f5;
	padding: 5px 10px;
}
.listBlockMax-infos {
	padding: 10px;
}
.twoLevel {overflow: hidden;}
.listBlockMax-infos .twoLevel .twoLevelIn{
	width: 50%;
	text-align: left;
	margin-left: 0;
	float: left;
	box-sizing:border-box;
}
.listBlockMax-infos .twoLevelIn > span {
	display: block;
	text-align: left;
	color: #010101;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.listBlockMax-infos .twoLevelIn > span:first-child {
