	border-color: #449d44;
	background-color: #449d44;
	margin-top: 28px;
	border-radius: 20px;
	font-size: 1.14287em;
	box-shadow: none;
}
.other-password-nav {
	text-align: right;
	overflow: hidden;
}
.other-password-left {
	float: left;
}
.other-password-a {
	color: #00caf2;
	text-decoration: none;
}
.mLoginForm-list {
	position: relative;
	padding-bottom: 10px;
}
.mLoginForm-list > i {
	position: absolute;
	top: 9px;
	right: 5px;
}
.mHeaderList > a {
	display: inline-block;
	width: 33.33333%;
	text-align: center;
	box-sizing:border-box;
	float: left;
	padding: .8em;
	color: #fff;
	font-size: 15px;
	text-decoration: none;
}
.mHeaderList > a > i {
	width: 52px;
	height: 52px;
	margin-left: auto;
	margin-right: auto;
