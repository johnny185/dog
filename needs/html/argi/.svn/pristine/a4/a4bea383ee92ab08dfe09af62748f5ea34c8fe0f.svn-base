}
.title-4 {
	padding:10px;
	clear: both;
}
.infosList li {
	margin-top:5px;
	margin-bottom: 10px;
	padding-bottom: 5px;
	border-bottom: 1px solid #e7e7e7;
}
.infosList li > a {
	position: relative;
	display: block;
	color: #a8a8a8;
	padding-right: 25px;
	text-align: right;
	text-decoration: none;
	font-size: 12px;
}
.infosListA:before,
.infosListA:after,
.rightArrow:before,
.rightArrow:after {
	position: absolute;	
	top: 50%;
	right: 5px;
	margin-top: -8px;
	border-style: solid;
	border-color: transparent;
	display: inline-block;
	width: 0;
	height: 0;
	content: "";
	clear: both;
}
.infosListA:before,
.rightArrow:before {
	border-width:8px;
	border-left-color: #ddd;
	z-index: 1;
}
.infosListA:after,
.rightArrow:after {
	right: 9px;
	margin-top: -6px;
	border-width:6px;
	border-left-color: #fff;
	z-index: 2;
}
.infosListA span:first-child + span {
	margin-left: 5px;
	float: left;
}
.infosListA span:first-child {
	display: block;
	text-align: left;
	width: 100%;
	margin-bottom: 6px;
	color: #010101;
	font-size: 14px;
}
.infosListA span:last-child{
	margin-right: 5px;
}
.view footer {
