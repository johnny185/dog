}
.backIcon{
	position: absolute;
	display: inline-block;
	height: 44px;
}
.leftArrow {
	position: relative;
	display: inline-block;
	height: 44px;
}
.leftArrow:before,
.leftArrow:after {
	position: absolute;
	top:50%;
	margin-top: -14px;
	display: inline-block;
	content: "";
	width: 0;
	height: 0;
	clear: both;
	border-color: transparent;
	border-style: solid;
}
.leftArrow:before {
	border-width: 14px;
	border-right-color: #fff;

}
.leftArrow:after{
	left:6px;
	margin-top: -12px;
	border-width: 12px;
	border-right-color: #66bb6a;
}
.titleTop {
	text-align: center;
	line-height: 44px;
	font-size: 18px;
	font-weight: 700;
	margin-top: 0;
	margin-bottom: 0;
}
.topTimes {
