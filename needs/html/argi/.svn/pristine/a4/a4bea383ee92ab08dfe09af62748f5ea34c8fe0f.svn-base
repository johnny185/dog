	position: relative;
	display: inline-block;
	float: right;
	line-height: 22px;
}
.editNumbNav > span:first-child {
	margin-left: 10px;
}
.btnLess{
	border-bottom-left-radius: 4px;
	border-top-left-radius: 4px;
}
.btnPlus {
	border-top-right-radius: 4px;
	border-bottom-right-radius: 4px;
}
.btnLess,
.btnPlus {
	position: relative;
	display: inline-block;
	width: 22px;
	height: 22px;
	background-color: #609eed;
	cursor: pointer;
	float: left;
}
.btnLess:before,
.btnPlus:before,
.btnPlus:after {
	position: absolute;
	left:50%;
	top: 50%;
	display: inline-block;
	content: "";
	background-color: #fff;
}
.btnLess:before,
.btnPlus:before {
	margin-left: -5px;
	margin-top: -1px;
	width: 10px;
	height: 2px;
}
.btnPlus:after{
	margin-left: -1px;
	margin-top: -5px;
	width: 2px;
	height: 10px;
}
input.lessPlusVal {
