html body{
	background-color: #fafafa;
	overflow-x:hidden;
	top: 0px;
}
/*banner*/
.banner-title{
	color: #fff;
}
.banner-title h5{
	font-size: 16px;
	font-weight: normal;
}


/*榜单*/
.top-container{
	position: relative;
	width: 100%;
	height: 170px;
	margin-top: 0px; 
	background-image: url("../image/background1.jpg");
	background-size:cover;
}
.profile{
	margin: 10px auto;
	width: 85px;
}
.profile-img{
	width: 85px;
	height: 85px;
	border-radius: 50%;
	border: 1px solid #fff;
/*	background-color: rgb(120, 175, 105);*/
	opacity: 1;
}
.name{
	color: #fff;
	opacity: 1.0;
	padding-top: 20px;
} 
.count{
	color: #fff;
	padding-top: 5px;
	font-size: 14px;
	font-weight: lighter;
}

/*累计部分*/
.total-container{
	height: 50px;
	border-bottom: 2px solid #f0f0f0;
}
.total-container h5{
	color: #66BB6A;
	font-weight: normal;
	font-size: 16px;
}
.total-container h6{
	margin: 10px auto 0px;
	color: #333;
}
.border-r{
	height: 50px;
	border-right: 2px solid #f0f0f0;
}
/*导航*/
.nav-container{
	padding-top: 20px; 
	position: relative;
	width: 100%;
	height: auto;
	background-color: #fafafa;
}
.nav-table{
	margin: 0px auto;
	width: 100%;
}
.nav-table td{
	text-align: center;
	width: 33.33333%;
}
.nav-table td img{
	width: 56px;
	height: 56px;
	margin: 10px auto;
}
.nav-table span{
	display: block;
	text-align: center;
	font-size: 12px;
	margin-top:  -8px;
	color: #999;
}
/*柱状图部分*/
.chart-title{
	margin: 15px auto 0px;
	line-height: 44px;
	height: 44px;
}
.chart-title h1{
	display: inline-block;
	float: left;
	width: 50%;
	font-size: 14px;
	text-align: center;
	color: #999;
}
.hr-l{
	width: 25%;
	display: inline-block;
	color: #999;
	margin-top: 22px;
}
.chart{
	width: 100%;
	height: auto;
}