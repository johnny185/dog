html, body{
	font-family: "Helvetica Neue", "Roboto", "Segoe UI", sans-serif;
	background-color: #fafafa;
	width: 100%;
	overflow-x:hidden;
	overflow-y: scroll;
	-ms-word-wrap: break-word; 
	word-wrap: break-word;
	-webkit-text-size-adjust: none; 
	text-size-adjust: none;
	}

#main-menu > div > a {
	width: 100%;
}

.height100{
	height: 100%;
}

/*-------Layout------------*/
.container{
	top:0px;
	margin-top: 0px;
}
.sub-container-notop{
	margin-top: 72px;
}
.sub-container-nobottom{
	margin-bottom: 44px;
	bottom: 44px;
}
.main{
	margin: 0px 20px;
	position:relative;
}
.main-10{
	margin: 0px 10px;
}
.full-main{
	margin: 0px;
}

/*--------features----------*/
.clear{
	clear: both;
}
.fixed{
	position: fixed;
}
.center{
	margin-left: auto;
	margin-right: auto;
	text-align: center;
}
.left{
	float: left;
}
.right{
	float: right;
}

/*--------banner----------*/
.banner{
	width: 100%;
	height: 72px;
	line-height: 50px;
	top: 0;
	z-index: 99999;
	border-bottom: 1px solid #bfbfbf;
	color: #333;
	background-color: #60bc56;
	padding-top:20px;
	box-sizing: border-box;
}
.banner-btn{
	/*border: 1px solid #fff;*/
	width: 20px;
	height: 20px;
	position: absolute;
	/*margin: -64px 0px 0px 20px; */
}
.back-btn{
	width: 20px;
	height: 20px;
	margin: 5px 0px 0px 20px;
}
.banner-btn-r{
	width: 20px;
	height: 20px;
	position: absolute;
	margin: -64px 20px 0px 88%; 
}
.banner-back-btn{
	top: 35px;
    left: 20px;
	width:10px;
	height:18px;
	background:url("../image/business_img/back.png") no-repeat;
	background-size:100% 100%;
}
.banner-edit-btn{
	background-image: url("../image/edit.png");
	background-size: 100%;
	background-repeat: none;
}
.banner-title{
	text-align: center;
	margin: auto;
}
.banner-title h4{
	color: #fff;
	font-size: 18px;
	font-weight: normal;
}
.notify-para{
	/*border: 1px solid red;*/
	width: 80%;
	margin: 40px auto -60px ;
}
.notify-para h4{
	padding-top: 20px;
	color: red;
}
/*--------banner2----------*/
.banner2{
	width: 100%;
	height: 44px;
	background: #fafafa;
	border-bottom: 1px solid #eee;
	line-height: 44px;
	top: 0;
	z-index: 99999;
}
/*--------banner-nocolor----------*/
.banner-nocolor{
	position: relative;
	width: 100%;
	height: 52px;
	padding-top: 8px;
	background: none
	border-bottom: 1px solid #eee;
	line-height: 44px;
	top: 0;
	z-index: 99999;
}
/*--------navbar----------*/
.nav{
	bottom: 0px;
	width: 100%;
	height: 44px;
	z-index: 99999;
	background-color: #fff;
}

/*---------menu-----------*/
