/*------------------------*/

input[type="button"], input[type="submit"], input[type="reset"],input[type="textarea"]{
	-webkit-appearance: none;
	-webkit-border: none;
	outline:none;
	resize: none;
}
input:focus{
	-webkit-appearance: none;
	-webkit-border: none;
	outline:none;
	resize: none;
}
input:active{
	-webkit-appearance: none;
	-webkit-border: none;
	outline:none;
	resize: none;
}
input[disabled]{
	color:black;opacity:0.8
}
textarea[disabled]{
	color:black;opacity:0.8
}

/*2017/6/14HU*/

.banner-right{
	position:absolute;
	right:10px;
	top:0;
}
.argi-body{font-size:14px;}
.argi-body,.padding{padding:.625em}
.argi-lists{padding-top:.625em;margin: 0 -.5em;}
.argi-body > .argi-lists:first-child{padding-top: .2em;}
.argi-lists-in{
	position:relative;
	background-color:#fff;
	padding: .7em .5em .2em;
	border-bottom:solid 1px #e5e5e5;
	display: table;
	width: 100%;

}
.argi-lists .argi-lists-in:first-child{border-top:solid 1px #e5e5e5;}
.argi-lists-in + .argi-lists-in{margin-top:0;}
.argi-lists-in div{
	line-height:2em;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.argi-lists-in div > span:first-child{color:#979797; margin-right:8px;}
/*.argi-lists-in div > span + span{margin-left:10px;}*/
.arrow-a{
	position: absolute;
	width: 18px;
	right: 4px;
	top: 50%;
	margin-top: -8px;
}
.container-panel{
	padding:0 .625em;
	background-color:#fff;
	margin: 0 -.5em;
	border-bottom: solid 1px #e5e5e5;
}
.argi-body > .container-panel:first-child{margin:-.5em -.5em 0;}
.infos-list-panel{background-color:#fff;}
.infos-panel-line{
	position:relative;
	display:table;
	padding:.5em 0;
	border-bottom:solid 1px #eceeeb;
} 
.infos-panel-line:after{
	content:"";
	display:block;
	line-height:0;
	overflow:hidden;
	clear:both;
}
.infos-panel-line.argi-flex-addr{display:block}
.infos-list-panel li.infos-panel-line:last-child{border-bottom:none;}
.text-right{
	text-align:right;
}
.argi-aborder{
	width:100%;
	border:solid 1px #e5e5e5;
	border-radius:4px;
	padding: .3125em;
	background-color:#fff;
}
/*按钮样式组*/
.argi-btn{
	display: inline-block;
	margin-bottom: 0;
	padding: .3125em .4em;
	vertical-align: middle;
	font-size: 1em;
	font-weight: 400;
	line-height: 20px;
	text-align: center;
	white-space: nowrap;
	background-image: none;
	border: 1px solid transparent;
	border-radius: 0;
	cursor: pointer;
	outline: 0;
	-webkit-appearance: none;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	-webkit-transition: background-color .3s ease-out,border-color .3s ease-out;
	transition: background-color .3s ease-out,border-color .3s ease-out;
}
.btn-full{width:100%;}
.btn-min-width{min-width:100px;}
.argi-btn-sm{
	padding-top:.1em;
	padding-bottom:.1em;
	font-size:12px;
}
.argi-btn-big{
	padding-top:.5em;
	padding-bottom:.5em;
	font-size:1.1em;
}
.argi-btn-default {
    color: #5eb95e;
    background-color: #fff;
    border-color: #e5e5e5;
}

.argi-btn-gray {
    color: #666;
    background-color: #e5e5e5;
    border-color: #e5e5e5;
}
.argi-btn-success{
	color: #fff;
	background-color: #5eb95e;
	border-color: #5eb95e;
}
.argi-btn-primary {
    color: #fff;
    background-color: #0e90d2;
    border-color: #0e90d2;
}
.argi-round{border-radius:1000px;}
.argi-btn-disabled{
	color: #b3b3b3 !important;
	background-color: #eeebeb !important;
	border-color: #eeebeb !important;
}

/*选项卡样式组 argi-tabs*/
.argi-tabs{
	width:100%;
}
.argi-tabs-nav{
	background-color:#9AD19A;
}
.argi-tabs-nav > li {
	position: relative;
    z-index: 1;
    float: left;
}
.argi-tabs-nav > li > a{
	color: #fff;
	background-color: #9AD19A;
	display:block;
	cursor:pointer;
	padding-top:.625em;
	padding-bottom:.625em;
}
.argi-tabs-nav > li.argi-active > a{
	background-color: #66BB6A;
}

.argi-nav:after, .argi-nav:before {
    content: " ";
    display: table;
}
.argi-nav:after {
    clear: both;
}
.even-block-2 > li{width:50%;}

/*表单样式组*/
.form-search-line{
	position:relative;
	padding-right:80px;
}
.form-search-line .argi-btn{position:absolute;top:0;right:0;}
.argi-form-control{
	width: 100%;
	border:solid 1px #0e90d2;
	vertical-align: middle;
	padding: .3125em;
	font-size: 1em;
	line-height: 20px;
	color: #555;
	background-color: #fff;
	background-image: none;
}
.argi-input-control,
.argi-textarea-control{
	display:inline-block;
	width: 100%;
	height:34px;
	border:solid 1px #e5e5e5;
	vertical-align: middle;
	padding: .3125em;
	font-size: 1em;
	line-height: 20px;
	color: #555;
	background-color: #fff;
	background-image: none;
}
.argi-textarea-control{height:auto;}
.argi-textarea-control{
	resize:none;
}
.select-form-control{
	display:inline-block;
	width: 100%;
	height:34px;
	border:solid 1px #e5e5e5;
	font-size: 1em;
	line-height: 20px;
	color: #555;
	background-color: #fff;
}



