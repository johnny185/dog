<script type="text/javascript">
var userId = null;
apiready = function(){
	userId = $api.getStorage("userId");
	
}

/* 退出   */
function logout(){
	api.confirm({
//	    title: '',
	    msg: "你确定要退出吗?",
	    buttons: ['确定', '取消']
	}, function(ret, err){
	    if( ret ){
	        if(ret.buttonIndex*1 == 1){
	        	//退出
	        	$api.setStorage("userId",null);					        
		    	$api.setStorage('argi_login_username',null);
				$api.setStorage('argi_login_password',null);
				
				api.closeToWin({
			        name: "login",
			        url:"../login/login.html",
			        slidBackEnabled:false,
			        reload:true,
			        animation:{
			            type:"movein",                //动画类型（详见动画类型常量）
			            subType:"from_right",       //动画子类型（详见动画子类型常量）
			            duration:300                //动画过渡时间，默认300毫秒
			        }
      			});
      			api.execScript({
				  	  name:'login',
				      //frameName:'harvest_workorder_list',
				      script: 'cleanAccount();'
				});
	        }
	    }
	});
}
function openChangePassword(){
		api.openWin({

         name: 'change_password',
         url: './mysettings/settings/change_password.html',
        reload:true,
        animation:{
            type:"movein",                //动画类型（详见动画类型常量）
            subType:"from_right",       //动画子类型（详见动画子类型常量）
            duration:300                //动画过渡时间，默认300毫秒
        }		             
	});

}
	

</script>
